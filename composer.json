{
    "name": "514sid/num",
    "license": "MIT",
    "type": "library",
    "description": "Num",
    "autoload": {
        "psr-4": {
            "Num\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1",
        "orchestra/testbench": "^8.8"
    },
    "scripts": {
        "tests": "phpunit",
        "post-autoload-dump": [
            "@php vendor/bin/testbench package:discover --ansi"
        ]
    }
}
